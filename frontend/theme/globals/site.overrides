.ui.segment.footer-wrapper {
  box-shadow: none;
  margin: 1rem 0em;
  padding: 0em;
  border: none;
}

.first-footer {
  .ui.segment.portal-footer {
      box-shadow: none;
      margin: 0em;
      padding: 0.5em 0 0 0.5em;
      border: none;
      background-color: #580D0D !important;
  }
  .ui.table.left-table {
    box-shadow: none;
    margin: 0em;
    padding: 0em;
    border: none;
    background-color: #580D0D !important;
    margin: 0 0 0 40px;

    a {
      color: #8FBCD8;
    }
    tr td {
      border: none !important;
    }

    tr th {
      background-color: transparent;
      border: none !important;
      font-weight: bold;
      color: #fff;
    }
  }
}

.footer-colophon {
  background: #222222 !important;
  color: #999999;
  padding: 1.5rem 0;
  text-align: center;
}
.colophon-item {
  display: inline-block;
  max-width: 46%;
  padding: 0 2%;
  vertical-align: top;
  position: relative;
}
.colophon-item a {
  color: #8FBCD8;
}
.colophon-info {
  font-size: 11px;
  margin-top: 1em;
}

